{
  "name": "ksu-robotics-website",
  "description": "Built with React, Graphql with headless WordPress, Apollo, and Express",
  "version": "0.1.0",
  "private": false,
  "dependencies": {
    "axios": "^0.18.0",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-helmet": "^5.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "build-css": "stylus src/assets/stylus/index.styl -o src",
    "css": "npm run build-css && stylus -w src/assets/stylus/index.styl -o src",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "server-js": "nodemon --watch server server.js",
    "start": "npm run start-js",
    "build": "npm run build-css && npm run build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-graphql": "^2.1.1",
    "stylus": "^0.54.5"
  }
}
